{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "entry",
        "targets": ">0.25%, not ie 11, not op_mini all"
      }
    ],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "babel-plugin-styled-components"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "useBuiltIns": "entry",
            "targets": ">0.25%, not ie 11, not op_mini all"
          }
        ],
        "@babel/react",
        "@babel/typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties", 
        "@babel/plugin-proposal-object-rest-spread",
        "babel-plugin-styled-components"
      ]
    }
  }
}
